[Service]
Description=Phonebot gateway
After=network.target
StartLimitIntervalSec=0[Service]
Type=simple
Restart=always
RestartSec=5
User=root
WorkingDirectory=/home/solin020/phonebot
ExecStart=/home/solin020/phonebot/env/bin/python -m uvicorn --host 0.0.0.0 --port 80 app:app

[Install]
WantedBy=multi-user.target
